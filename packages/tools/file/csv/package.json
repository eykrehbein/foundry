{
    "name": "@usefoundry/tools-file-csv",
    "description": "Tool for working with CSV files in Foundry",
    "version": "0.1.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "author": "Foundry Community",
    "license": "MIT",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "postbuild": "yarn generate-docs",
        "generate-docs": "node ./node_modules/@usefoundry/utils/dist/cli.js generate-docs --type file",
        "release": "yarn build && changeset publish",
        "lint": "tsc",
        "test": "vitest"
    },
    "devDependencies": {
        "tsup": "^7.0.0",
        "typescript": "^5.1.3",
        "vitest": "^0.32.2",
        "@changesets/cli": "^2.26.2"
    },
    "dependencies": {
        "@usefoundry/tsconfig": "*",
        "@usefoundry/utils": "*",
        "csv": "^6.3.1",
        "csv-parse": "^5.4.0",
        "csv-stringify": "^6.4.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
